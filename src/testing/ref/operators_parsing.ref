PROGRAM { [STMT {DECASSIGN {VDECL {num, a} = NUMLIT {10.}}}, STMT {DECASSIGN {VDECL {num, b} = NUMLIT {5.}}}, STMT {DECASSIGN {VDECL {num, c} = NUMLIT {2.}}}, STMT {DECASSIGN {VDECL {num, result} = BINOP {BINOP {ID {a} ADD BINOP {ID {b} TIMES ID {c}}} SUB BINOP {BINOP {ID {a} DIV ID {c}} MOD ID {b}}}}}, IF {BINOP {ID {result} NEQ NUMLIT {20.}}, BLOCK {STMT {CALL {print, [STRINGLIT {Error in operator precedence!}]}}}, BLOCK {STMT {CALL {print, [STRINGLIT {Operator precedence good!}]}}}}, STMT {DECASSIGN {VDECL {num, mult} = BINOP {ID {b} TIMES ID {c}}}}, STMT {DECASSIGN {VDECL {num, div} = BINOP {ID {a} DIV ID {b}}}}, STMT {DECASSIGN {VDECL {num, mod} = BINOP {ID {a} MOD ID {c}}}}, STMT {DECASSIGN {VDECL {num, add} = BINOP {ID {mult} ADD ID {div}}}}, STMT {DECASSIGN {VDECL {num, sub} = BINOP {ID {a} SUB ID {mod}}}}, IF {BINOP {BINOP {BINOP {BINOP {BINOP {ID {mult} NEQ NUMLIT {10.}} OR BINOP {ID {div} NEQ NUMLIT {2.}}} OR BINOP {ID {mod} NEQ NUMLIT {0.}}} OR BINOP {ID {add} NEQ NUMLIT {12.}}} OR BINOP {ID {sub} NEQ NUMLIT {10.}}}, BLOCK {STMT {CALL {print, [STRINGLIT {Error in basic operations!}]}}}, BLOCK {STMT {CALL {print, [STRINGLIT {Basic operations good!}]}}}}] }
