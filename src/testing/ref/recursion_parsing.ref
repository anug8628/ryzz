PROGRAM { [FUNC_DEF {rtype: {num}, fname: {fib}, formals: { [BIND {num, a}] }, body: { [IF {BINOP {ID {a} EQUAL NUMLIT {1.}}, BLOCK {RETURN {NUMLIT {1.}}}, IF {BINOP {ID {a} EQUAL NUMLIT {0.}}, BLOCK {RETURN {NUMLIT {1.}}}, BLOCK {RETURN {BINOP {CALL {fib, [BINOP {ID {a} SUB NUMLIT {1.}}]} ADD CALL {fib, [BINOP {ID {a} SUB NUMLIT {2.}}]}}}}}}] }, IF {BINOP {BINOP {BINOP {CALL {fib, [NUMLIT {1.}]} NEQ NUMLIT {1.}} OR BINOP {CALL {fib, [NUMLIT {2.}]} NEQ NUMLIT {2.}}} OR BINOP {CALL {fib, [NUMLIT {6.}]} NEQ NUMLIT {13.}}}, BLOCK {STMT {CALL {print, [STRINGLIT {problem with recursion!}]}}}, BLOCK {STMT {CALL {print, [STRINGLIT {recursion good!}]}}}}] }
