PROGRAM { [STMT {DECASSIGN {VDECL {num, a} = NUMLIT {1.}}}, STMT {DECASSIGN {VDECL {num, b} = NUMLIT {3.}}}, STMT {DECASSIGN {VDECL {num, i} = NUMLIT {0.}}}, FUNC_DEF {rtype: {num}, fname: {hello}, formals: { [BIND {num, a}] }, body: { [STMT {ASSIGN {a = NUMLIT {3.}}}, WHILE {BINOP {ID {i} LT NUMLIT {4.}}, BLOCK {STMT {DECASSIGN {VDECL {num, b} = ID {i}}}, STMT {ASSIGN {i = BINOP {ID {i} ADD NUMLIT {1.}}}}}}, RETURN {BINOP {ID {a} ADD ID {b}}}] }, IF {BINOP {CALL {hello, [ID {a}]} NEQ NUMLIT {6.}}, BLOCK {STMT {CALL {print, [STRINGLIT {Error!}]}}}, BLOCK {}}, IF {BINOP {ID {a} NEQ NUMLIT {1.}}, BLOCK {STMT {CALL {print, [STRINGLIT {Error!}]}}}, BLOCK {}}] }
