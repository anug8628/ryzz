func fizzbuzz(num n, func () -> none fizz, func () -> none buzz) -> none {
    for (num i = 1; i <= n; i = i + 1) {
        if (i % 3 == 0 and i % 5 == 0) {
            fizz();
            buzz();
        } 
        else if (i % 3 == 0) {
            fizz();
        }
        else if (i % 5 == 0) {
            buzz();
        } else {}
    }
}